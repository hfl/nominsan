<p id="notice"><%= notice %></p>

<h1>Creators</h1>

<table>
  <thead>
    <tr>
      <th>Literature</th>
      <th>Creator</th>
      <th>Role</th>
      <th>Institution</th>
      <th>Discipline</th>
      <th>Authorid</th>
      <th>Intr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creators.each do |creator| %>
      <tr>
        <td><%= creator.literature %></td>
        <td><%= creator.creator %></td>
        <td><%= creator.role %></td>
        <td><%= creator.institution %></td>
        <td><%= creator.discipline %></td>
        <td><%= creator.authorid %></td>
        <td><%= creator.intr %></td>
        <td><%= link_to 'Show', creator %></td>
        <td><%= link_to 'Edit', edit_creator_path(creator) %></td>
        <td><%= link_to 'Destroy', creator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creator', new_creator_path %>
