<p id="notice"><%= notice %></p>

<h1>Format Mimes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @format_mimes.each do |format_mime| %>
      <tr>
        <td><%= format_mime.name %></td>
        <td><%= format_mime.code %></td>
        <td><%= format_mime.about %></td>
        <td><%= link_to 'Show', format_mime %></td>
        <td><%= link_to 'Edit', edit_format_mime_path(format_mime) %></td>
        <td><%= link_to 'Destroy', format_mime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Format Mime', new_format_mime_path %>
