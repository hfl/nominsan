<p id="notice"><%= notice %></p>

<h1>Tocs</h1>

<table>
  <thead>
    <tr>
      <th>Literature</th>
      <th>Chapter</th>
      <th>Title</th>
      <th>Page</th>
      <th>Fiel number</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tocs.each do |toc| %>
      <tr>
        <td><%= toc.literature %></td>
        <td><%= toc.chapter %></td>
        <td><%= toc.title %></td>
        <td><%= toc.page %></td>
        <td><%= toc.fiel_number %></td>
        <td><%= toc.parent_id %></td>
        <td><%= link_to 'Show', toc %></td>
        <td><%= link_to 'Edit', edit_toc_path(toc) %></td>
        <td><%= link_to 'Destroy', toc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Toc', new_toc_path %>
